package e105;

import java.util.Scanner;

public class Main
{

	public static void main(String[] args)
	{
		Scanner sc = new Scanner(System.in);
		String[] dias = {"MARTES", "MIERCOLES", "JUEVES", "VIERNES", "SABADO", "DOMINGO"};

		boolean hayDatos = true;
		float totalRecaudacion;
		float min;
		float max;
		int indiceDia = 0;
		int iMax = 0;
		int iMin = 0;
		float recaudacion;
		int numDia = 0;
		float[] recaudaciones = new float[6];
		while(hayDatos)
		{
			max = 0;
			min = Float.MAX_VALUE;
			totalRecaudacion = 0;
			recaudacion = sc.nextFloat();
			if(recaudacion != -1)
			{
				recaudaciones[0] = recaudacion;
				totalRecaudacion += recaudacion;
				for (int i = 0; i < 5; i++)
				{
					recaudaciones[i + 1] = sc.nextFloat();
				}
				System.out.println(totalRecaudacion);
			}
		}

	}

}
