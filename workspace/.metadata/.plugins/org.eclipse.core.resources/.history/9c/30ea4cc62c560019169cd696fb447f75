package hashset;

public class Empleado
{
	private String nombre;
	private double sueldo;
	public Empleado(String nombre, double sueldo)
	{
		super();
		this.nombre = nombre;
		this.sueldo = sueldo;
	}
	public String getNombre()
	{
		return nombre;
	}
	public double getSueldo()
	{
		return sueldo;
	}

}
