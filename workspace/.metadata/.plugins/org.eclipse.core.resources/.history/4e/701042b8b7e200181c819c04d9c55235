package aleatorios;

import java.util.Random;

public class Main
{

	public static void main(String[] args)
	{
		Random random = new Random();
		int resultado;
		for (int i = 0; i < 15; i++)
		{
			resultado = random.nextInt(3);
			switch (resultado)
			{
				case 0:
				System.out.println("1");
				break;
				case 1:
				System.out.println(" X");
				break;
				case 2:
				System.out.println("  2");
				break;
			}
		}

	}

}
